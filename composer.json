        "composer/class-map-generator": "^1.3",
        "illuminate/contracts": "10.*|11.*",
        "illuminate/support": "10.*|11.*"
    },
    "require-dev": {
        "orchestra/testbench": "9.1.*",
        "phpunit/phpunit": "11.1.*"
    },
    "autoload": {
        "psr-4": {
            "DeGraciaMathieu\\RapidBind\\": "src/"
        }
